<div>
  <div class="container">
    <h1>メッセージ</h1>
    <% @rooms.each do |room| %>
    <tr>
      <p><%= link_to room_path(room.user) do %>
      <%= attachment_image_tag room.user, :profile_image, :fill, 50, 50, fallback: "profile/no_image.jpg",  size: "50x50" %><br>
      <%= room.user.name %>
      <% end %></p>
      <%= (room.room.messages.last).content %><br>
      <%= (room.room.messages.last).created_at.strftime("%Y/%m/%d %H:%M") %>
    </tr>
    <% end %>
  </div>
</div>