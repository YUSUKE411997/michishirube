<div>
  <div class="container">
    <h1>投稿詳細画面</h1>
    <table class="table">
      <td><%= @post.user.name %></td>
      <td><%= @post.type %></td>
      <td><%= @post.title %></td>
      <td><%= @post.body %></td>
      <td><%= post_created(@post) %></td>
      <td><%= link_to "投稿を削除", admin_post_path(@post.id), method: :delete, "data-confirm" => "本当に削除しますか？", class: "btn btn-outline-danger" %></td>
    </table>
  </div>
</div>